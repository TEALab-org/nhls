            }
            if (t1%2 == 0) {
              A[0][(-2*t3+t4-1)] = (exp(-0.01*t3)/2)*(((-2*t3+t4-1) == N-1)?0:A[1][(-2*t3+t4-1)+1]) + (1-(exp(-0.01*t3)))*A[1][(-2*t3+t4-1)] + (exp(-0.01*t3)/2)*(((-2*t3+t4-1) == 0)?0:A[1][(-2*t3+t4-1)-1]);;
            }
          }
        }
      }
      if (t1 <= min(floord(2048*t2-N,1024),floord(2048*t2+2*T-N-2048,2048))) {
        if (N%2 == 0) {
          A[1][(N-1)] = (exp(-0.01*((2048*t1-2048*t2+N+2046)/2))/2)*(((N-1) == N-1)?0:A[0][(N-1)+1]) + (1-(exp(-0.01*((2048*t1-2048*t2+N+2046)/2))))*A[0][(N-1)] + (exp(-0.01*((2048*t1-2048*t2+N+2046)/2))/2)*(((N-1) == 0)?0:A[0][(N-1)-1]);;
        }
      }
    }
  }
}

#ifdef TIME
    gettimeofday(&end, 0);

    ts_return = timeval_subtract(&result, &end, &start);
    tdiff = (double)(result.tv_sec + result.tv_usec * 1.0e-6);

    printf("|Time taken =  %7.5lfs\t", tdiff);
#endif

#ifdef VERIFY
    total=0;
    for (i = 0; i < N; i++) {
        total+= A[T%2][i] ;
    }
    printf("|sum: %e\t", total);
    for (i = 0; i < N; i++) {
        sum_err_sqr += (A[T%2][i] - (total/N))*(A[T%2][i] - (total/N));
    }
    printf("|rms(A) = %7.2f\t", sqrt(sum_err_sqr));
    for (i = 0; i < N; i++) {
        chtotal += ((char *)A[T%2])[i];
    }
    printf("|sum(rep(A)) = %ld\n", chtotal);
#endif
    return 0;
}

